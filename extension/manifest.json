{
  "manifest_version": 3,
  "name": "CanaryBlock",
  "description": "CanaryBlock",
  "version": "1.0",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}